{
  "name": "andrey-crm-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "server": "json-server --watch server/db.json --port 3001"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "google",
      "react-app/jest"
    ],
    "rules": {
      "no-invalid-this": 0,
      "require-jsdoc": 0,
      "linebreak-style": 0,
      "indent": [
        "error",
        4
      ],
      "max-len": [
        "error",
        120
      ],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1
        }
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^27.5.1"
  }
}
